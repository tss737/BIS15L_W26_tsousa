library("tidyverse")
name <- c("Everest", "K2", "Kangchenjunga", "Lhotse", "Makalu")
height_ft <- c(29032,28251,28169,27940,27838)
height_m <- c(8849,8611,8586,8516,8485)
mountains <- tibble(name, height_ft, height_m)
mean(mountains$height_ft)
first_climbed <- c(1953, 1954, 1955, 1956, 1955)
mountains <- tibble(name, height_ft, height_m, first_climbed)
summits <- c(12884, 800, 532, 933, 499)
mountains <- tibble(name, height_ft, height_m, first_climbed, summits)
fatalities <- c(340, 96, 52, 20, 72)
mountains <- tibble(name, height_ft, height_m, first_climbed, summits, fatalities)
fatality_rate <- fatalities / summits
mountains <- tibble(name, height_ft, height_m, first_climbed, summits, fatalities, fatality_rate)
View(mountains)
write.csv(mountains, "mountains_data.csv", row.names = FALSE)
mountains <- read_csv("mountains_data.csv")
View(mountains)
summary(mountains)
